{
	"name": "vite-react-starter",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"preview": "vite preview",
		"build:dev": "tsc && vite build --mode dev",
		"build:iqa": "tsc && vite build --mode iqa",
		"build:qa": "tsc && vite build --mode qa",
		"build:uat": "tsc && vite build --mode uat",
		"build:iprod": "tsc && vite build --mode iprod",
		"build:prod": "tsc && vite build --mode prod",
		"format": "npx prettier --write .",
		"lint": "npx eslint --ext .tsx --ext .ts src --fix",
		"prepare": "husky install"
	},
	"dependencies": {
		"@mantine/core": "7.0.0-beta.6",
		"@mantine/dates": "7.0.0-beta.6",
		"@mantine/form": "7.0.0-beta.6",
		"@mantine/hooks": "7.0.0-beta.6",
		"@tabler/icons-react": "^2.34.0",
		"@tanstack/react-query": "^4.35.3",
		"axios": "^1.5.0",
		"dayjs": "^1.11.9",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.16.0",
		"vite-plugin-svgr": "^3.2.0",
		"vite-tsconfig-paths": "^4.2.1",
		"zustand": "^4.4.1"
	},
	"devDependencies": {
		"@tanstack/eslint-plugin-query": "^4.34.1",
		"@types/node": "^20.6.2",
		"@types/react": "^18.2.21",
