		"prop-types": "^15.8.1",
		"tailwindcss": "^3.3.3",
		"tailwindcss-touch": "^1.0.1",
		"typescript": "^5.2.2",
		"vite": "^4.4.9",
		"vite-plugin-eslint": "^1.8.1"
